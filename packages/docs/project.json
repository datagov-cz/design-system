{
	"name": "docs",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/docs",
	"projectType": "library",
	"generators": {},
	"tags": [],
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npm run docs:build",
				"cwd": "packages/docs"
			},
			"dependsOn": ["docs:readmes"]
		},
		"serve": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npm run docs:dev",
				"cwd": "packages/docs"
			},
			"dependsOn": ["docs:readmes"]
		},
		"readmes": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npm run assets:copy-readmes",
				"cwd": "packages/docs"
			},
			"dependsOn": ["core:build", "styles:build"]
		}
	}
}
